{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Registro Schema",
	"description": "Modelo de Registro",
	"type": "object",
	"properties": {
		"username": {
			"type": "string",
			"pattern": "^[A-z0-9 .]+@[A-z0-9 .]+$"
		},
		"password": {
			"type": "string"
		}
	},
	"required": ["username", "password"],
	"additionalProperties": false
}